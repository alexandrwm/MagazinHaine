</head>

<body id="page-top">
    <div id="wrapper">
        @Html.Partial("_Header")

        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    @Html.Partial("_Header1")
                </nav>

                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>

            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>&copy; 2024 - Universitatea Tehnica a Moldovei - <a href=@Url.Action("Privacy", "Home")>Privacy</a></span>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
         aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Iesire">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Selectați „Deconectare” de mai jos dacă sunteți gata să încheiați sesiunea curentă.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Anulare</button>
                    <a class="btn btn-primary" href=@Url.Action("Logout", "Home")>Ieșire</a>
                </div>
            </div>
        </div>
    </div>

    <script src="~/wwwroot/AdminTemplate/vendor/jquery/jquery.min.js"></script>
    <script src="~/wwwroot/AdminTemplate/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/wwwroot/AdminTemplate/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/wwwroot/AdminTemplate/js/sb-admin-2.min.js"></script>
    <script src="~/wwwroot/AdminTemplate/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/wwwroot/AdminTemplate/vendor/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="~/wwwroot/AdminTemplate/js/demo/datatables-demo.js"></script>
    <script src="~/wwwroot/js/jquery.datetimepicker.full.min.js"></script>
    @*@await RenderSectionAsync("Scripts", required: false)*@
</body>

</html>